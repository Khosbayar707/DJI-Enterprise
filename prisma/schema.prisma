generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique @default(nanoid())
  email String  @unique
  password String
  isAdmin Boolean @default(false)
  sub_news Boolean @default(false)
  isActive Boolean @default(true)
}